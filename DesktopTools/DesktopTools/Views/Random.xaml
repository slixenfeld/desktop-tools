<Window x:Class="DesktopTools.Views.Random"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopTools.Views"
        mc:Ignorable="d"
        Title="Random" Height="330" Width="270" Icon="/Views/dice.png" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox x:Name="ResultBox" HorizontalAlignment="Left" Margin="10,218,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="213" Height="60"/>
        <TextBox x:Name="InputBox" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="213" Height="60"/>
        <Button x:Name="MD5Button" Content="MD5" HorizontalAlignment="Left" Height="22" Margin="10,75,0,0" VerticalAlignment="Top" Width="49" Click="MD5_Click"/>

    </Grid>
</Window>
